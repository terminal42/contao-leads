{
    "name":"terminal42/contao-leads",
    "description":"Leads extension for Contao Open Source CMS; Store and manage form data with ease!",
    "keywords":["contao", "form", "data", "store", "export"],
    "type":"contao-bundle",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"terminal42 gmbh",
            "homepage":"http://terminal42.ch"
        }
    ],
    "require":{
        "php":"^7.1 || ^8.0",
        "symfony/console": "^3.4 || ^4.0",
        "symfony/filesystem": "^3.4 || ^4.0",
        "contao/core-bundle":"^4.6",
        "codefog/contao-haste": "^4.11.4",
        "menatwork/contao-multicolumnwizard": "^3.3"
    },
    "require-dev": {
        "phpoffice/phpexcel": "^1.8",
        "terminal42/notification_center": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "suggest": {
        "phpoffice/phpexcel": "To enable excel export of lead data.",
        "terminal42/notification_center": "Send notifications for leads."
    },
    "autoload":{
        "psr-4": {
            "Terminal42\\LeadsBundle\\": "src"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\LeadsBundle\\ContaoManager\\Plugin"
    }
}
