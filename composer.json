{
    "name": "terminal42/contao-leads",
    "description": "Leads extension for Contao Open Source CMS; Store and manage form data with ease!",
    "keywords": ["contao", "form", "data", "store", "export"],
    "type": "contao-bundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "https://www.terminal42.ch"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/terminal42"
        },
        {
            "type": "other",
            "url": "https://ko-fi.com/terminal42"
        }
    ],
    "support": {
        "issues": "https://github.com/terminal42/contao-leads/issues",
        "source": "https://github.com/terminal42/contao-leads",
        "forum": "https://community.contao.org"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "codefog/contao-haste": "^5.0",
        "contao/core-bundle": "^4.13 || ^5.0",
        "doctrine/dbal": "^2.11 || ^3.0",
        "menatwork/contao-multicolumnwizard": "^3.3",
        "symfony/config": "^4.4 || ^5.4 || ^6.0",
        "symfony/console": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.4 || ^6.0",
        "symfony/event-dispatcher-contracts": "^1.1 || ^2.0 || ^3.0",
        "symfony/filesystem": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.4 || ^6.0",
        "symfony/routing": "^4.4 || ^5.4 || ^6.0",
        "symfony/security-bundle": "^4.4 || ^5.4 || ^6.0"
    },
    "require-dev": {
        "phpoffice/phpexcel": "^1.8",
        "terminal42/notification_center": "^1.7",
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "suggest": {
        "phpoffice/phpexcel": "To enable excel export of lead data.",
        "terminal42/notification_center": "Send notifications for leads."
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\LeadsBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\LeadsBundle\\ContaoManager\\Plugin"
    },
    "scripts": {
        "cs-fixer": "@php tools/ecs/vendor/bin/ecs check src/ --fix --ansi"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true,
            "contao-community-alliance/composer-plugin": true
        }
    }
}
